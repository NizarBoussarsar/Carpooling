{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Villes</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Codepostal</th>
                <td>{{ entity.codePostal }}</td>
            </tr>
            <tr>
                <th>Localite</th>
                <td>{{ entity.localite }}</td>
            </tr>
            <tr>
                <th>Delegation</th>
                <td>{{ entity.delegation }}</td>
            </tr>
            <tr>
                <th>Gouvernorat</th>
                <td>{{ entity.gouvernorat }}</td>
            </tr>
            <tr>
                <th>Latitude</th>
                <td>{{ entity.latitude }}</td>
            </tr>
            <tr>
                <th>Longitude</th>
                <td>{{ entity.longitude }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('villes') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('villes_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
